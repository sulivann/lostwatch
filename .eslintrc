{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "prettier/standard"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "no-return-assign": "off",
    "jsx-a11y/click-events-have-key-events": 0
  },
  "env": {
    "browser": true
  }
}
