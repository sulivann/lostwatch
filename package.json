{
  "name": "lostwatch",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/build.js && node scripts/server.js",
    "dev": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "server": "node scripts/server.js",
    "lint:js": "eslint src/**/*.js; exit 0",
    "lint:js-fix": "eslint src/**/*.js --fix",
    "lint:scss": "stylelint",
    "lint:staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commit-master/corobizar-v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/commit-master/corobizar-v2/issues"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.17.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "http-auth": "^3.2.3",
    "jest": "20.0.4",
    "lint-staged": "^4.3.0",
    "moment": "^2.20.1",
    "node-sass": "^4.6.0",
    "object-assign": "4.1.1",
    "parallax-js": "^3.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "pre-commit": "^1.2.2",
    "prettier": "^1.7.4",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-paginate": "^5.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scrollable-anchor": "^0.6.1",
    "react-sound": "^1.1.0",
    "react-tabs": "^2.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-scss": "^2.1.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "empty": "^0.10.1",
    "redux-devtools": "^3.4.0",
    "stylelint-config-standard": "^17.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "src/**/*.{scss,css}": "lint:scss"
  },
  "pre-commit": {
    "run": [
      "lint:staged"
    ],
    "colors": true,
    "silent": true
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.3",
    "utf-8-validate": "^4.0.0"
  }
}
